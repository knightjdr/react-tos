{
  "name": "react-tos",
  "version": "0.1.0",
  "description": "React component for transitioning elements on scroll",
  "main": "build/index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "demo": "node scripts/demo.js",
    "deploy": "git subtree push --prefix demo origin gh-pages",
    "minor-release": "npm version minor && npm publish && git push --tags",
    "patch-release": "npm version patch && npm publish && git push --tags",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knightjdr/react-tos.git"
  },
  "author": "James Knight <knightjdr@gmail.com>",
  "bugs": {
    "url": "https://github.com/knightjdr/react-tos/issues"
  },
  "homepage": "https://github.com/knightjdr/react-tos#readme",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "dotenv": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "promise": "^8.1.0",
    "raf": "^3.4.1",
    "react": "^16.14.0",
    "react-dev-utils": "^4.2.3",
    "react-dom": "^16.14.0",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-manifest-plugin": "^1.3.2",
    "whatwg-fetch": "^2.0.4"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
